{
  "expo": {
    "name": "INDX",
    "slug": "INDX",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/logos.png",
    "splash": {
      "image": "./src/assets/SplashLogo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.indx.doctor"
    },
    "web": {
      "favicon": "./src/assets/favicon.png"
    },
    "android": {
      "package": "com.indx.doctor"
    }
  }
}